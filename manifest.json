{
  "manifest_version": 2,

  "name": "Jira Sprint Collapser",
  "description": "This extension collapses all of the sprints in the Jira backlog",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background":{
      "persistent": false,
      "scripts":["background.js"]
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
